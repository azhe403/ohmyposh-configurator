[
  {
    "type": "fossil",
    "name": "Fossil",
    "description": "Displays Fossil repository information",
    "icon": "Bone",
    "defaultTemplate": " {{ .Branch }} ",
    "defaultForeground": "#ffffff",
    "defaultBackground": "#556270",
    "previewText": "trunk",
    "properties": [
      {
        "name": ".Branch",
        "type": "string",
        "description": "current branch"
      },
      {
        "name": ".Status.Changed",
        "type": "boolean",
        "description": "if the status contains changes"
      }
    ]
  },
  {
    "type": "git",
    "name": "Git",
    "description": "Displays Git repository information",
    "icon": "GitMerge",
    "defaultTemplate": "{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}",
    "defaultForeground": "#ffffff",
    "defaultBackground": "#98C379",
    "previewText": "main ✓",
    "properties": [
      {
        "name": ".RepoName",
        "type": "string",
        "description": "the repo folder name"
      },
      {
        "name": ".HEAD",
        "type": "string",
        "description": "the current HEAD context"
      },
      {
        "name": ".Working.Changed",
        "type": "boolean",
        "description": "if the worktree has changes"
      },
      {
        "name": ".Staging.Changed",
        "type": "boolean",
        "description": "if the staging area has changes"
      },
      {
        "name": ".BranchStatus",
        "type": "string",
        "description": "the current branch context"
      }
    ]
  },
  {
    "type": "gitversion",
    "name": "GitVersion",
    "description": "Displays GitVersion semantic version",
    "icon": "Tag",
    "defaultTemplate": " {{ .Major }}.{{ .Minor }}.{{ .Patch }} ",
    "defaultForeground": "#ffffff",
    "defaultBackground": "#4EC9B0",
    "previewText": "1.2.3",
    "properties": [
      {
        "name": ".Major",
        "type": "string",
        "description": "major number"
      },
      {
        "name": ".Minor",
        "type": "string",
        "description": "minor number"
      },
      {
        "name": ".Patch",
        "type": "string",
        "description": "patch number"
      }
    ]
  },
  {
    "type": "jujutsu",
